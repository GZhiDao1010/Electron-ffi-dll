{
  "name": "electron-fii-dll",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . app --win --out presenterTool --arch=x64 --app--version 1.0.0  --overwrite --ignore=node_modules --icon=./public/ico/icon_256_exe.ico",
    "postinstall": "electron-rebuild install-app-deps",
    "rebuild": "cd ./node_modules/.bin&& electron-rebuild --force --module-dir = .. / .. /"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "chobyn",
  "license": "CC0-1.0",
  "devDependencies": {},
  "dependencies": {
    "bindings": "^1.3.0",
    "electron": "^2.0.9",
    "ffi": "^2.2.0",
    "iconv-lite": "^0.4.24"
  }
}
